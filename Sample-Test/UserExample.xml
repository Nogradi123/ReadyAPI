  <con:testStep type="groovy" name="Groovy Script - Check Price" id="2d0c68e9-36e6-4291-8d82-a9cc66661705">
    <con:settings/>
    <con:config>
      <script>//Create variable called limit, set to 2.00 
def limit = 2.00

//Use GETDATA to retrieve the price value from the Search Item response
def item_price = context.expand( '${Search Item#Response#$[\'result\'][\'item\'][\'price\']}' )

//check if price is less than limit. 
// -- if less or equal, route test case to Buy Item.
// -- else, skip to delay. 

if ( item_price.toFloat() &lt;=  limit){
	testRunner.gotoStepByName( "Buy Item")
}else{
	testRunner.gotoStepByName( "Delay")
}</script>
    </con:config>
  </con:testStep>
