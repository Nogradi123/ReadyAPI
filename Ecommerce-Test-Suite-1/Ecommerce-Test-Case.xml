        <con:assertion type="Response SLA Assertion" id="709f89d1-a1ba-4305-8ac6-0f04868506b6" name="Response SLA">
          <con:configuration>
            <SLA>30</SLA>
          </con:configuration>
        </con:assertion>
        <con:assertion type="DataAndMetadataAssertion" id="67879509-3166-43c2-9e1a-b99e94831c8e" name="Smart Assertion">
          <con:configuration xsi:type="con:DataAndMetadataAssertion">
            <con:mode>Received Data</con:mode>
            <con:elements>
              <con:path>$['purchasestatus']['id']</con:path>
              <con:element>id</con:element>
              <con:operatorId>1</con:operatorId>
              <con:enabled>true</con:enabled>
              <con:expectedValue>Item 6</con:expectedValue>
              <con:caseSensitive>true</con:caseSensitive>
              <con:numberType>false</con:numberType>
              <con:expectedDataType>1</con:expectedDataType>
            </con:elements>
            <con:elements>
              <con:path>$['purchasestatus']['stockStatus']</con:path>
              <con:element>stockStatus</con:element>
              <con:operatorId>1</con:operatorId>
              <con:enabled>true</con:enabled>
              <con:expectedValue>1 license</con:expectedValue>
              <con:caseSensitive>true</con:caseSensitive>
              <con:numberType>false</con:numberType>
              <con:expectedDataType>1</con:expectedDataType>
            </con:elements>
            <con:elements>
              <con:path>$['purchasestatus']['expectedDelivery']</con:path>
              <con:element>expectedDelivery</con:element>
              <con:operatorId>1</con:operatorId>
              <con:enabled>true</con:enabled>
              <con:expectedValue>You should already have it.</con:expectedValue>
              <con:caseSensitive>true</con:caseSensitive>
              <con:numberType>false</con:numberType>
              <con:expectedDataType>1</con:expectedDataType>
            </con:elements>
            <con:originalMessage>{
  "purchasestatus": {
    "id": "Item 5",
    "stockStatus": "1 license",
    "expectedDelivery": "You should already have it."
  }
}</con:originalMessage>
          </con:configuration>
        </con:assertion>
